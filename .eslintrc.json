{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "plugins": ["react"],
    "rules": {
        "react/jsx-filename-extension": [
            2,
            {
                "extensions": [".js", ".jsx"]
            }
        ],
        "react/forbid-prop-types": [0],
        "react/require-default-props": [0],
        "react/jsx-indent": [2, 4],
        "react/jsx-indent-props": [2, 4],
        "indent": ["error", 4],
        "global-require": [0],
        "linebreak-style": "off"
    }
}
